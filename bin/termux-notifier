#!/usr/bin/env bash

read -r -a cmd

if [[ "$1" = "success" ]]; then
    title="zsh-notify: ${cmd[0]} success"
    msg="Successfully run '${cmd[0]}', took $2"
else
    title="zsh-notify: ${cmd[0]} error"
    msg="Command ${cmd[0]} return non-zero exit code, took $2"
fi

termux-run termux-notification \

